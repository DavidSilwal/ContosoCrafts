{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Steeltoe": "Information"
    }
  },
  "consul": {
    "host": "consul_service",
    "port": 8500,
    "discovery": {
      "enabled": "true",
      "scheme": "http",
      "port": 80,      
      "serviceName": "contoso-productsapi",
      "tags": ["api", "contoso", "products", "aspnetcore"],
      "healthCheckPath": "/health",
      "heartbeat": {
        "enabled": false
      },
      "register": true,
      "deregister": true
    }
  },
  "AllowedHosts": "*"
}
