{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithEnvironmentUserName"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Level:u3}]: {Timestamp:dd-MM-yyyy:HH:mm:ss} {SourceContext} {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Properties": {
      "ApplicationName": "Contoso Website"
    }
  },
  "Orleans": {
    "ClusterId": "contoso-crafts-cluster",
    "ServiceId": "contoso-silo",
    "ConsulCluster": {
      "Address": "http://localhost:8500",
      "KvRootFolder": "contoso"
    },
    "RedisCluster": {
      "Host": "localhost",
      "Port": 6379,
      "Password": "S0m3P@$$w0rd"
    }
  },
  "AllowedHosts": "*"
}
